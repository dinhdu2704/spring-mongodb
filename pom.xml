        <relativePath/>
    </parent>

    <groupId>vn.vnpt.stc.enterprise</groupId>
    <artifactId>core-log</artifactId>
    <version>1.0-SNAPSHOT</version>

    <name>stc-enterprise-core-log</name>
    <description>ONE Home Core Log</description>

    <properties>
        <java.version>1.8</java.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>vn.vnpt.stc.enterprise</groupId>
